{
  "name": "Live CricScore",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension is created to get the live cricket score",
  "homepage_url": "https://www.github.com",
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo.png"
  },
  "default_locale": "en",
  "background": 
    {
        
        "scripts": ["js/jquery/jquery.min.js","src/bg/background.js"]
    },
  "browser_action": {
    "default_icon": "icons/logo16.png"
  },
  "permissions": [
    "clipboardRead",
    "contentSettings",
    "notifications"
  ]
}
